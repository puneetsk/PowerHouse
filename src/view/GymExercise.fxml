                  <Font name="Calisto MT" size="14.0" />
               </font>
            </Button>
            <Label layoutX="272.0" layoutY="233.0" text="ExID" textFill="#737eee">
               <font>
                  <Font name="Calisto MT Bold" size="24.0" />
               </font>
            </Label>
            <Label layoutX="272.0" layoutY="275.0" text="Name" textFill="#737eee">
               <font>
                  <Font name="Calisto MT Bold" size="24.0" />
               </font>
            </Label>
            <Label layoutX="272.0" layoutY="317.0" text="Program" textFill="#737eee">
               <font>
                  <Font name="Calisto MT Bold" size="24.0" />
               </font>
            </Label>
            <TextField fx:id="txtexeid" layoutX="387.0" layoutY="238.0" promptText="Enter the Exid to Delete" />
