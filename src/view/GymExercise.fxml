<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.GymExerciseController">
   <children>
      <AnchorPane id="logbg" prefHeight="500.0" prefWidth="600.0" stylesheets="@../../style.css">
         <children>
            <Button fx:id="addExe" layoutX="315.0" layoutY="399.0" mnemonicParsing="false" onAction="#add" prefHeight="35.0" prefWidth="63.0" text="Add">
