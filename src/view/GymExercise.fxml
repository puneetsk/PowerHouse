               </font>
            </Button>
            <Button fx:id="delExe" layoutX="466.0" layoutY="399.0" mnemonicParsing="false" onAction="#delete" prefHeight="35.0" prefWidth="69.0" text="Delete">
               <font>
                  <Font name="Calisto MT" size="14.0" />
               </font>
            </Button>
            <TextField fx:id="txtexeprogram" layoutX="387.0" layoutY="321.0" />
            <Button layoutX="475.0" layoutY="43.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="82.0" text="Clear Fields" />
